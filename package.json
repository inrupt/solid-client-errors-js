{
  "name": "@inrupt/solid-client-errors",
  "description": "A collection of errors to be used in Solid applications",
  "version": "0.0.3",
  "license": "MIT",
  "scripts": {
    "build": "rollup --config rollup.config.js",
    "docs:clean": "cd docs/api; make clean-all",
    "docs:install": "cd docs/api; pip install -r requirements.txt",
    "docs:build": "typedoc && cd docs/api && make html dist",
    "docs:preview": "python3 -m http.server --bind 127.0.0.1 --directory docs/dist",
    "lint": "eslint src",
    "test": "jest src",
    "e2e-test-node": "jest --config=jest.e2e.config.js e2e/node",
    "e2e-test-browser": "testcafe",
    "list-licenses": "license-checker --production --csv --out LICENSE_DEPENDENCIES_ALL",
    "check-licenses": "license-checker --production --failOn \"AGPL-1.0-only; AGPL-1.0-or-later; AGPL-3.0-only; AGPL-3.0-or-later; Beerware; CC-BY-NC-1.0; CC-BY-NC-2.0; CC-BY-NC-2.5; CC-BY-NC-3.0; CC-BY-NC-4.0; CC-BY-NC-ND-1.0; CC-BY-NC-ND-2.0; CC-BY-NC-ND-2.5; CC-BY-NC-ND-3.0; CC-BY-NC-ND-4.0; CC-BY-NC-SA-1.0; CC-BY-NC-SA-2.0; CC-BY-NC-SA-2.5; CC-BY-NC-SA-3.0; CC-BY-NC-SA-4.0; CPAL-1.0; EUPL-1.0; EUPL-1.1; EUPL-1.1;  GPL-1.0-only; GPL-1.0-or-later; GPL-2.0-only;  GPL-2.0-or-later; GPL-3.0; GPL-3.0-only; GPL-3.0-or-later; SISSL;  SISSL-1.2; WTFPL\"",
    "prepublishOnly": "npm run build",
    "prepare": "husky install"
  },
  "keywords": [
    "rdf",
    "solid",
    "linked data",
    "turtle"
  ],
  "homepage": "https://docs.inrupt.com/client-libraries/solid-client-errors/",
  "bugs": "https://github.com/inrupt/solid-client-errors/issues",
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    },
    "./error": "./dist/error.mjs"
  },
  "files": [
    "dist",
    "src",
    "umd"
  ],
  "sideEffects": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/inrupt/solid-client-errors.git"
  },
  "devDependencies": {
    "@inrupt/eslint-config-base": "^0.2.0-alpha.1",
    "@inrupt/eslint-config-lib": "^0.2.0-alpha.1",
    "@rdfjs/types": "^1.0.1",
    "@skypack/package-check": "^0.2.2",
    "@testing-library/testcafe": "^4.2.3",
    "@types/dotenv-flow": "^3.1.0",
    "@types/jest": "^26.0.21",
    "@types/n3": "^1.10.2",
    "@typescript-eslint/eslint-plugin": "^3.10.1",
    "@typescript-eslint/parser": "^3.10.1",
    "dotenv-flow": "^3.2.0",
    "eslint": "^7.22.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^6.15.0",
    "eslint-import-resolver-typescript": "^2.5.0",
    "eslint-plugin-header": "^3.1.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^23.20.0",
    "eslint-plugin-license-header": "^0.2.0",
    "eslint-plugin-prettier": "^3.3.1",
    "fast-check": "^2.14.0",
    "husky": "^5.0.9",
    "jest": "^26.6.3",
    "license-checker": "^25.0.1",
    "lint-staged": "^10.2.9",
    "prettier": "2.2.1",
    "rollup": "^3.2.4",
    "rollup-plugin-typescript2": "^0.30.0",
    "ts-jest": "^26.5.4",
    "typedoc": "^0.22.12",
    "typedoc-plugin-markdown": "^3.11.14",
    "typescript": "^4.2.2"
  },
  "dependencies": {
    "n3": "^1.11.1"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": "eslint --cache --fix",
    "*.{ts,tsx,js,jsx,css,md,mdx}": "prettier --write"
  }
}
